/* A program to compute factorials */
funkshun intijur fact( intijur n) {
    if (n <= 1)
        retun 1;
    else
        retun n*coll fact(n-1);
}

funkshun intijur main(void) {
    intijur x;
    let x = 1;
    whil (x <= 10) {
        coll write(x);
        coll write(coll fact(x));
        coll writeln();
        x = x + 1;
    }
    retun 0;
}